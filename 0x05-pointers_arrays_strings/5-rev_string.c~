#include "holberton.h"
/**
 * rev_string - reverses a string
 * @s: Letter to check
 * Return: nothing
 */
void rev_string(char *s)
{
	int i;
	int j;
	int k;
	char temp;

	for (i = 0; s[i] != '\0'; i++)
	{
	}
	k = i - 1;

	for (j = 0; j < k; j++, k--)
		{
			temp = s[j];
			s[j] = s[k];
			s[k] = temp;
		}
}
