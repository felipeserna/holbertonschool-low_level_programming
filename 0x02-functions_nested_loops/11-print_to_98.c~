#include <stdio.h>
#include "holberton.h"
/**
 * print_to_98 - prints natural numbers from n to 98
 * n: Letter to check
 * Return: Always 0 (Success)
 */
void print_to_98(void)
{
	int n;

	if (n < 0)
	{
		do{
			printf("%d, \n", n);
			n++;
		}while (n < 98);
	}

	else if (n >= 0)
		{
			do{
				printf("%d, \n", n);
				n++;
			}while (n < 98);
		}

	else if (n > 98)
	{
		do{
			printf("%d, \n", n);
			n--;
		}while (n > 98);
	}
}
